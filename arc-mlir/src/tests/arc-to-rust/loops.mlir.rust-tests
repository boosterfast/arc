#[cfg(test)]
mod tests {
  use crate::arctorustloops::*;
  use hexf::*;
  #[test]
  fn it_works() {
    assert_eq!(a_while_loop(0, 5, 3), 13);
    assert_eq!(a_while_loop_with_a_break_in_before(0, 5, 1), 2);
    assert_eq!(a_while_loop_with_a_break_in_after(0, 10, 3), 4);
    assert_eq!(a_while_loop_with_a_return_in_before(0, 5, 1), 2);
    assert_eq!(a_while_loop_with_a_return_in_after(0, 10, 3), 4);
  }
}
